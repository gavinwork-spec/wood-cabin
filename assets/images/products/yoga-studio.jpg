<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <!-- Sky background -->
  <rect width="800" height="600" fill="#87CEEB"/>

  <!-- Grass -->
  <rect x="0" y="400" width="800" height="200" fill="#228B22"/>

  <!-- Large Studio Cabin -->
  <rect x="150" y="250" width="500" height="200" fill="#8B4513" stroke="#654321" stroke-width="3"/>

  <!-- Roof -->
  <polygon points="120,250 400,130 680,250" fill="#A0522D" stroke="#654321" stroke-width="3"/>

  <!-- Double Doors -->
  <rect x="360" y="370" width="80" height="80" fill="#654321"/>
  <line x1="400" y1="370" x2="400" y2="450" stroke="#4A3018" stroke-width="2"/>
  <circle cx="385" cy="410" r="4" fill="#FFD700"/>
  <circle cx="415" cy="410" r="4" fill="#FFD700"/>

  <!-- Large Studio Windows -->
  <rect x="180" y="290" width="140" height="90" fill="#87CEEB" stroke="#654321" stroke-width="2"/>
  <line x1="250" y1="290" x2="250" y2="380" stroke="#654321" stroke-width="2"/>
  <line x1="180" y1="335" x2="320" y2="335" stroke="#654321" stroke-width="2"/>

  <rect x="480" y="290" width="140" height="90" fill="#87CEEB" stroke="#654321" stroke-width="2"/>
  <line x1="550" y1="290" x2="550" y2="380" stroke="#654321" stroke-width="2"/>
  <line x1="480" y1="335" x2="620" y2="335" stroke="#654321" stroke-width="2"/>

  <!-- Art equipment visible -->
  <rect x="200" y="360" width="100" height="30" fill="#654321" opacity="0.7"/>
  <!-- Easels -->
  <rect x="220" y="300" width="3" height="60" fill="#8B4513" opacity="0.7"/>
  <rect x="240" y="300" width="3" height="60" fill="#8B4513" opacity="0.7"/>
  <rect x="580" y="300" width="3" height="60" fill="#8B4513" opacity="0.7"/>
  <rect x="600" y="300" width="3" height="60" fill="#8B4513" opacity="0.7"/>

  <!-- Path -->
  <rect x="390" y="450" width="20" height="100" fill="#D2691E"/>

  <!-- Large trees -->
  <circle cx="80" cy="370" r="60" fill="#228B22"/>
  <rect x="60" y="430" width="40" height="70" fill="#8B4513"/>

  <circle cx="720" cy="370" r="60" fill="#228B22"/>
  <rect x="700" y="430" width="40" height="70" fill="#8B4513"/>

  <!-- Sun -->
  <circle cx="100" cy="80" r="40" fill="#FFD700"/>

  <!-- Text -->
  <text x="400" y="550" font-family="Arial, sans-serif" font-size="28" fill="#FFFFFF" text-anchor="middle" font-weight="bold">Large Studio Cabin</text>
</svg>