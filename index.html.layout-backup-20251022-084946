        // Function to create homepage testimonial HTML
        function createHomepageTestimonialCard(testimonial) {
            return `
                <article class="testimonial-card">
                    <div class="testimonial-header">
                        <div class="client-info">
                            <div class="client-avatar">
                                <i class="fas fa-user-tie" aria-hidden="true"></i>
                            </div>
                            <div>
                                <h4>${testimonial.name}</h4>
                                <p class="client-title">${testimonial.title}</p>
                                <p class="client-company">${testimonial.company}</p>
                            </div>
                        </div>
                        <div class="testimonial-rating" aria-label="5 star rating">
                            <i class="fas fa-star" aria-hidden="true"></i>
                            <i class="fas fa-star" aria-hidden="true"></i>
                            <i class="fas fa-star" aria-hidden="true"></i>
                            <i class="fas fa-star" aria-hidden="true"></i>
                            <i class="fas fa-star" aria-hidden="true"></i>
                        </div>
                    </div>
                    <div class="testimonial-content">
                        <p>"${testimonial.content}"</p>
                    </div>
                    <div class="testimonial-project">
                        <i class="fas fa-project-diagram" aria-hidden="true"></i>
                        <strong>Project:</strong> ${testimonial.project}
                    </div>
                </article>
            `;
        }

        // Function to get random homepage testimonials
        function getRandomHomepageTestimonials(count) {
            const shuffled = [...homepageTestimonials].sort(() => 0.5 - Math.random());
            return shuffled.slice(0, count);
        }

        // Function to load homepage testimonials
        function loadHomepageTestimonials() {
            const container = document.getElementById('homepage-testimonials-container');
            if (!container) return;

            // Get 4 random testimonials (2 rows of 2)
            const selectedTestimonials = getRandomHomepageTestimonials(4);

            // Clear container and add testimonials
            container.innerHTML = '';
            selectedTestimonials.forEach(testimonial => {
                container.innerHTML += createHomepageTestimonialCard(testimonial);
            });
        }

        // Initialize all components when DOM is loaded
        document.addEventListener('DOMContentLoaded', () => {
            new MobileMenu();
            new ScrollAnimations();
            new ProductFilter();

            // Load random homepage testimonials
            loadHomepageTestimonials();

            // Initialize carousels
            const productsCarousel = new Carousel('products-carousel', {
                autoplay: true,
                interval: 6000
            });

            const testimonialsCarousel = new Carousel('testimonials-carousel', {
                autoplay: true,
                interval: 7000
            });

            // Smooth scrolling for anchor links
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    const target = document.querySelector(this.getAttribute('href'));
                    if (target) {
                        target.scrollIntoView({
                            behavior: 'smooth',
                            block: 'start'
                        });
                    }
                });
            });

            // Performance monitoring
            if ('performance' in window) {
                window.addEventListener('load', () => {
                    setTimeout(() => {
                        const perfData = performance.getEntriesByType('navigation')[0];
                        console.log(`Page loaded in ${perfData.loadEventEnd - perfData.startTime}ms`);
                    }, 0);
                });
            }
        });

        // Error handling for missing images
        document.addEventListener('DOMContentLoaded', () => {
            // Images are loaded normally - no placeholder fallbacks needed
        });
    </script>

